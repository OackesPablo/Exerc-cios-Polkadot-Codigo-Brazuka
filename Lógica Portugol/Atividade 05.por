programa {
  funcao inicio() {
    real temp, fahrenheith
    escreva("Convertendo o calor da mulestia de Celsius para Fahrenheith\n")

    escreva(">>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>\n")

   escreva("Informe a temperatura em Celsius:")
   leia (temp)

    fahrenheith = (temp *9/5)+32
  
    escreva("A temperatura em Fahrenheith é:", fahrenheith)
  }
}
