programa { 
  funcao inicio() {  
    real num1, num2, num3, media

    escreva("Digita o primeiro número meu consagrado:")
    leia(num1)

    escreva("Agora digite o segundo número meu rei:")
    leia(num2)

    escreva("Por fim, digite o terceiro e último número:")
    leia(num3)

    media = ((num1 + num2 + num3) /3)

    escreva("A média final dos números digitados é:", +media)
  }
}
